                                                              https://agw.imgix.net/{{ path }}?auto=format&dpr=2&w=890&h=380&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 2x" media="(min-width: 1536px)">
  <source srcset="https://agw.imgix.net/{{ path }}?auto=format&w=890&h=700&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 1x,
                                                              https://agw.imgix.net/{{ path }}?auto=format&dpr=2&w=890&h=400&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 2x" media="(min-width: 1280px)">
  <source srcset="https://agw.imgix.net/{{ path }}?auto=format&w=890&h=200&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 1x,
                                                              https://agw.imgix.net/{{ path }}?auto=format&dpr=2&w=890&h=400&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 2x" media="(min-width: 1024px)">
  <source srcset="https://agw.imgix.net/{{ path }}?auto=format&w=890&h=400&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 1x,
                                                              https://agw.imgix.net/{{ path }}?auto=format&dpr=2&w=890&h=400&fit=crop{% if page.url contains '/new/' %}&bri=-10&sat=-100{% endif %} 2x" media="(min-width: 769px)">
